         </GridPane.margin>
      </Button>
      <Button fx:id="g2" mnemonicParsing="false" onAction="#g2Pressed" text="G2 to green" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
