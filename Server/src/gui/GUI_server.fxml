      </Button>
      <TextField fx:id="ramzorText" maxWidth="79.0" minWidth="79.0" prefWidth="79.0" promptText="num of ramzor" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
